
@{
    ViewBag.Title = "Projects";
}


<table border="1">

    <tr style="border-bottom: solid 1px black;">
        <th style=" border-left: 1px solid black; padding : 5px;">
            Course
        </th>
        <th style=" border-left: 1px solid black; padding: 5px;">
            Title
        </th>
        <th style=" border-left: 1px solid black; padding: 5px;">
            Description
        </th>
    </tr>



    @for (int i = 0; i < ViewBag.projectsArray.GetLength(0); i++)
    {
        <tr style="border-bottom: solid 1px black;">
            @for (int j = 0; j < ViewBag.projectsArray.GetLength(1); j++)
            {
                <td style=" border-left: 1px solid black; padding: 5px;">@ViewBag.projectsArray[i, j]</td>
            }
        </tr>
    }

</table>